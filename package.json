{
  "name": "kern",
  "version": "0.1.3",
  "module": "src/index.tsx",
  "type": "module",
  "license": "GPL-3.0-only",
  "scripts": {
    "dev": "bun run --watch src/index.tsx",
    "start": "bun run src/index.tsx",
    "build": "bun build --compile src/index.tsx --outfile dist/kern --define \"KERN_VERSION='$(jq -r .version package.json)'\" --define \"KERN_COMPILED='true'\"",
    "generate:schema": "bun run src/lib/generate-schema.ts"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@opentui/core": "^0.1.77",
    "@opentui/react": "^0.1.77",
    "react": "^19.2.4"
  }
}
